<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/register6"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Register6Activity">

    <ProgressBar
        android:id="@+id/progressBar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="367dp"
        android:layout_height="18dp"
        android:layout_marginTop="28dp"
        android:max="110"
        android:paddingTop="3dp"
        android:paddingBottom="3dp"
        android:progress="60"
        android:progressDrawable="@drawable/rounded_corners_progressbar"
        android:progressTint="#A084DC"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonback"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="36dp"
        android:background="@drawable/button_with_border_back"
        android:text="Back"
        android:textColor="@color/med_purple"
        android:padding="10dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.098"
        app:layout_constraintStart_toStartOf="parent"
        android:textAllCaps="false"
        />

    <Button
        android:id="@+id/buttonmatched"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="36dp"
        android:text="Next"
        android:background="@drawable/radius50_button"
        android:textColor="@color/purple_white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.905"
        app:layout_constraintStart_toStartOf="parent"
        android:textAllCaps="false"
        />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="367dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Please upload your face picture"
        android:textColor="@color/dark_gray"
        android:textSize="@dimen/big_32"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/progressBar"
        />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="367dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/spam"
        android:textSize="@dimen/small_12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        android:textColor="@color/gray"
        />

    <com.google.android.flexbox.FlexboxLayout
        android:id="@+id/linearLayout3"
        android:layout_width="367dp"
        android:layout_height="179dp"
        android:layout_marginTop="28dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        app:justifyContent="space_between"
        app:alignItems="center">

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/minions"
            tools:layout_editor_absoluteX="158dp"
            tools:layout_editor_absoluteY="297dp"
            />

        <TextView
            android:id="@+id/plus_icon4"
            android:layout_width="113dp"
            android:layout_height="163dp"
            android:background="@drawable/rounded_background_pg6"
            android:gravity="center"
            android:text="+"
            android:textColor="#000000"
            android:textSize="48sp" />
        <TextView
            android:id="@+id/plus_icon"
            android:layout_width="113dp"
            android:layout_height="163dp"
            android:background="@drawable/rounded_background_pg6"
            android:gravity="center"
            android:text="+"
            android:textColor="#000000"
            android:textSize="48sp" />


    </com.google.android.flexbox.FlexboxLayout>

    <com.google.android.flexbox.FlexboxLayout
        android:layout_width="367dp"
        android:layout_height="179dp"
        android:layout_marginTop="28dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
        app:justifyContent="space_between"
        app:alignItems="center">

        <TextView
            android:id="@+id/plus_icon1"
            android:layout_width="113dp"
            android:layout_height="163dp"
            android:background="@drawable/rounded_background_pg6"
            android:gravity="center"
            android:text="+"
            android:textColor="#000000"
            android:textSize="48sp" />
        <TextView
            android:id="@+id/plus_icon2"
            android:layout_width="113dp"
            android:layout_height="163dp"
            android:background="@drawable/rounded_background_pg6"
            android:gravity="center"
            android:text="+"
            android:textColor="#000000"
            android:textSize="48sp" />
        <TextView
            android:id="@+id/plus_icon3"
            android:layout_width="113dp"
            android:layout_height="163dp"
            android:background="@drawable/rounded_background_pg6"
            android:gravity="center"
            android:text="+"
            android:textColor="#000000"
            android:textSize="48sp" />
    </com.google.android.flexbox.FlexboxLayout>


</androidx.constraintlayout.widget.ConstraintLayout>